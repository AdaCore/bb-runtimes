<?xml version="1.0"?>
<multi-image arch="aarch64" pageshift="12">
  <!--  Definition of the memory map -->
  <include file="../../arm/rpi2/memmap.xml"/>

  <core>
    <area size="32MB" region="ram"/>
  </core>
  
  <partitions>
    <partition name="part1">
      <area name="RAM" size="128MB" region="ram" vaddr="0x40000000"/>
      <file filename="../linux/mc.dtb" name="dtb"/>
      <file filename="../linux/Image" name="linux"/>
    </partition>
  </partitions>
</multi-image>
